	.FUNCT	RT-I-HUNGER:ANY:0:0
	INC	'GL-HUNGER
	CRLF	
	EQUAL?	GL-HUNGER,2 \?CCL3
	ADD	GL-MOVES,120
	ICALL	RT-QUEUE,RT-I-HUNGER,STACK
