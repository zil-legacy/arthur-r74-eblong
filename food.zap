	JUMP	?CND11
?CCL13:	EQUAL?	HERE,RM-ABOVE-BOG \?CCL15
	PRINTI	"into the bog"
	JUMP	?CND11
?CCL15:	PRINTI	"to the ground"
?CND11:	PRINTI	" below, killing yourself."
	CRLF	
	JUMP	?CND8
?CCL10:	PRINTI	"You collapse from hunger.
"
?CND8:	CALL1	RT-END-OF-GAME
	RSTACK	


	.FUNCT	RT-I-SLEEP:ANY:0:0,L,CAP?,N,?TMP1
	SET	'CAP?,TRUE-VALUE
