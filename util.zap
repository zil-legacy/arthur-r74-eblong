	PRINTI	"zero"
	RTRUE	
?CCL12:	EQUAL?	COUNT,1 \?CCL14
	PRINTI	"one"
	RTRUE	
?CCL14:	EQUAL?	COUNT,2 \?CCL16
	PRINTI	"two"
	RTRUE	
