	ICALL1	V-HINT
	IN?	CH-PLAYER,RM-CRYSTAL-CAVE /?PRG17
	ZERO?	M? /?PRG17
	ZERO?	MSG? /?PRG17
	PRINTI	"Your vision clears and Merlin says, ""Welcome back, Arthur.""
"
	JUMP	?PRG17
?CCL73:	ZERO?	M? /?CND86
	PRINTC	34
?CND86:	PRINTI	"Please type "
	ZERO?	P-CAN-UNDO /?CND88
	PRINTI	"UNDO, "
