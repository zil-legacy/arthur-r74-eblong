	INTBL?	HERE,?TMP1,STACK,1 \?CCL11
	PRINTI	"You hear Merlin's voice faintly in your ears, ""You have failed, Arthur. England is doomed."""
	CRLF	
	CRLF	
	ICALL1	V-SCORE
	CRLF	
	JUMP	?PRG17
?CCL11:	SET	'M?,TRUE-VALUE
	PRINTI	"Merlin "
	IN?	CH-MERLIN,HERE /?CND12
