	DIV	COUNT,100
	ICALL	RT-WORD-NUMBERS,STACK,FALSE-VALUE
	PRINTI	" hundred"
	MOD	COUNT,100
	GRTR?	STACK,0 \FALSE
	PRINTI	" and "
	MOD	COUNT,100
	CALL	RT-WORD-NUMBERS,STACK,FALSE-VALUE
