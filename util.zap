	.FUNCT	V-COLOR:ANY:0:0,S,BG,FG
	EQUAL?	GL-MACHINE,APPLE-2C,APPLE-2E,APPLE-2GS \?CCL3
	PRINTR	"Sorry, the Apple II doesn't support color selection."
?CCL3:	ZERO?	GL-COLOR-NOTE \?CND1
	EQUAL?	GL-MACHINE,MACINTOSH \?CND1
	CALL1	MAC-II?
	ZERO?	STACK /?CND1
	SET	'GL-COLOR-NOTE,TRUE-VALUE
	PRINTI	"If your Mac II displays fewer than 256 colors, you probably won't get the color you ask for. Do you still want to go ahead"
	PRINTI	"Do you still want to go ahead?"
	CRLF	
	CALL1	Y?
	ZERO?	STACK /TRUE
?CND1:	CRLF	
	SET	'FG,GL-F-COLOR
	SET	'BG,GL-B-COLOR
?PRG10:	ICALL1	RT-DO-COLOR
	PRINTI	"You should now get "
	GET	K-COLOR-TABLE,GL-F-COLOR
	PRINT	STACK
	PRINTI	" text on a "
	GET	K-COLOR-TABLE,GL-B-COLOR
	PRINT	STACK
	PRINTI	" background. Is that what you want?
"
	CALL1	Y?
	ZERO?	STACK \?REP11
	EQUAL?	GL-MACHINE,MACINTOSH \?CND14
	CALL1	MAC-II?
	ZERO?	STACK \?CND14
	EQUAL?	GL-B-COLOR,2 \?CCL20
	SET	'GL-B-COLOR,9
	SET	'GL-F-COLOR,2
	JUMP	?REP11
?CCL20:	SET	'GL-B-COLOR,2
	SET	'GL-F-COLOR,9
	JUMP	?REP11
?CND14:	PRINTI	"
Do you want to pick again, or would you like to just keep the current colors? (Type Y to pick again)>"
	CALL2	Y?,FALSE-VALUE
	ZERO?	STACK /?CCL23
	CRLF	
	JUMP	?PRG10
?CCL23:	SET	'GL-F-COLOR,FG
	SET	'GL-B-COLOR,BG
?REP11:	ICALL1	RT-COLOR-ALL-WINDOWS
	CALL1	V-$REFRESH
	RSTACK	


	.FUNCT	RT-COLOR-ALL-WINDOWS:ANY:0:0,S
	SET	'S,0
?PRG1:	SCREEN	S
	COLOR	GL-F-COLOR,GL-B-COLOR
	IGRTR?	'S,7 \?PRG1
	RTRUE	


	.FUNCT	RT-DO-COLOR:ANY:0:0
	GET	0,8
	BTST	STACK,64 \?CTR2
	EQUAL?	GL-MACHINE,MACINTOSH \?CCL3
	CALL1	MAC-II?
	ZERO?	STACK \?CCL3
?CTR2:	EQUAL?	GL-B-COLOR,2 \?CCL10
	SET	'GL-B-COLOR,9
	SET	'GL-F-COLOR,2
	RETURN	GL-F-COLOR
?CCL10:	SET	'GL-B-COLOR,2
	SET	'GL-F-COLOR,9
	RETURN	GL-F-COLOR
?CCL3:	CALL	RT-PICK-COLOR,GL-F-COLOR,STR?63,TRUE-VALUE >GL-F-COLOR
	CALL	RT-PICK-COLOR,GL-B-COLOR,STR?64 >GL-B-COLOR
	RETURN	GL-B-COLOR


	.FUNCT	RT-PICK-COLOR:ANY:2:3,WHICH,STRING,SETTING-FG,CHAR
	PRINTI	"The current "
	PRINT	STRING
	PRINTI	" color is "
	GET	K-COLOR-TABLE,WHICH
	PRINT	STACK
	PRINTC	46
	CRLF	
	FONT	4
	PRINTI	"   1 --> WHITE   5 --> YELLOW"
	CRLF	
	PRINTI	"   2 --> BLACK   6 --> BLUE"
	CRLF	
	PRINTI	"   3 --> RED     7 --> MAGENTA"
	CRLF	
	PRINTI	"   4 --> GREEN   8 --> CYAN"
	CRLF	
	FONT	1
	PRINTI	"Type a number to select the "
	PRINT	STRING
	PRINTI	" color you want.>"
?PRG1:	ZERO?	DEMO-VERSION? /?CCL5
	CALL2	INPUT-DEMO,1 >CHAR
	JUMP	?CND3
?CCL5:	INPUT	1 >CHAR
?CND3:	SUB	CHAR,48 >CHAR
	EQUAL?	CHAR,1 \?CND6
	SET	'CHAR,9
?CND6:	EQUAL?	CHAR,2,3,4 /?CTR9
	EQUAL?	CHAR,5,6,7 /?CTR9
	EQUAL?	CHAR,8,9 \?CCL10
?CTR9:	ZERO?	SETTING-FG \?REP2
	EQUAL?	CHAR,GL-F-COLOR \?REP2
	PRINTI	"
You can't make the background the same color as the text. Please pick another color.>"
	JUMP	?PRG1
?CCL10:	CRLF	
	PRINT	K-TYPE-NUMBER-MSG
	PRINTI	"8.>"
	JUMP	?PRG1
?REP2:	CRLF	
	CRLF	
	RETURN	CHAR


