	CLEAR	5
	CLEAR	6
	ADD	W,1 >?PR-X
?PRG6:	IGRTR?	'?PR-Y,WY \?CND8
	SET	'?PR-Y,1
	ADD	?PR-X,W
	ADD	STACK,2 >?PR-X
	ADD	?PR-X,W
	ADD	STACK,1
	GRTR?	STACK,WX /?REP7
?CND8:	ICALL	CCURSET,?PR-Y,?PR-X
	HLIGHT	K-H-INV
	PRINTC	32
	HLIGHT	K-H-NRM
