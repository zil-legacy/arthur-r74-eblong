	SET	'GL-WIN-X,X
	SET	'GL-WIN-Y,Y
?CND26:	SUB	WY,GL-WIN-Y
	ADD	GL-WIN-N,STACK
	ADD	STACK,1 >GL-WIN-N
	ADD	GL-WIN-X,W
	ADD	STACK,2 >GL-WIN-X
	SET	'GL-WIN-Y,1
?CND22:	EQUAL?	GL-MACHINE,APPLE-2C,APPLE-2E,APPLE-2GS \?CCL37
	PUSH	STR?224
	JUMP	?CND35
?CCL37:	PUSH	STR?225
?CND35:	ICALL	RT-PUT-UP-FOO,STACK,TRUE-VALUE,TRUE-VALUE
	CALL2	RT-PUT-UP-OBJS,TRUE-VALUE
	ZERO?	STACK \?CND38
	ICALL	RT-PUT-UP-FOO,STR?223,FALSE-VALUE,TRUE-VALUE
?CND38:	SET	'N,GL-WIN-N
	SET	'X,GL-WIN-X
	SET	'Y,GL-WIN-Y
	DIV	GL-WIN-N,WY >?TMP1
	DIV	K,WY
	LESS?	?TMP1,STACK \?CND40
	SUB	WY,GL-WIN-Y
	ADD	GL-WIN-N,STACK
	ADD	STACK,1 >GL-WIN-N
	ADD	GL-WIN-X,W
	ADD	STACK,2 >GL-WIN-X
	SET	'GL-WIN-Y,1
?CND40:	SET	'I,GL-WIN-N
?PRG42:	LESS?	I,K \?REP43
