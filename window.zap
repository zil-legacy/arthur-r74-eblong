	SET	'GL-WIN-Y,1
	SET	'GL-WIN-X,1
	EQUAL?	GL-MACHINE,APPLE-2C,APPLE-2E,APPLE-2GS \?CCL14
	PUSH	STR?221
	JUMP	?CND12
?CCL14:	PUSH	STR?222
?CND12:	ICALL	RT-PUT-UP-FOO,STACK,TRUE-VALUE,TRUE-VALUE
	CALL2	RT-PUT-UP-OBJS,FALSE-VALUE
	ZERO?	STACK \?CND15
	ICALL	RT-PUT-UP-FOO,STR?223,FALSE-VALUE,TRUE-VALUE
?CND15:	SET	'N,GL-WIN-N
	SET	'X,GL-WIN-X
	SET	'Y,GL-WIN-Y
	SET	'I,GL-WIN-N
?PRG17:	LESS?	I,J \?REP18
	ICALL2	RT-PUT-UP-FOO,FALSE-VALUE
	INC	'I
	JUMP	?PRG17
?REP18:	SET	'GL-WIN-N,N
	SET	'GL-WIN-X,X
	SET	'GL-WIN-Y,Y
	SET	'J,GL-WIN-N
	GRTR?	GL-WIN-Y,1 \?CND22
	MUL	W,2
	ADD	GL-WIN-X,STACK
	ADD	STACK,1
	GRTR?	STACK,WX /?CND22
	LESS?	J,K \?CND26
	DIV	K,WY >?TMP1
	DIV	J,WY
	EQUAL?	?TMP1,STACK \?CND26
	SET	'N,GL-WIN-N
	SET	'X,GL-WIN-X
	SET	'Y,GL-WIN-Y
	SET	'I,GL-WIN-N
?PRG30:	LESS?	I,K \?REP31
	ICALL2	RT-PUT-UP-FOO,FALSE-VALUE
	INC	'I
	JUMP	?PRG30
