	.FUNCT	RT-UPDATE-MAP-WINDOW:ANY:0:1,REF?,TBL,PTR,Y,X,?TMP2,?TMP1
	ZERO?	GL-MAP-GRID-Y \?CND1
	PICINF	K-MAP-GRID-SIZE,K-WIN-TBL /?BOGUS3
?BOGUS3:	GET	K-WIN-TBL,0 >GL-MAP-GRID-Y
