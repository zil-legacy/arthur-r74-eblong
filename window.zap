?PRG8:	CALL	RT-GET-NEXT-DIR,HERE,DIR,TRUE-VALUE >DIR
	ZERO?	DIR /TRUE
	EQUAL?	DIR,P?IN,P?OUT /?PRG8
	SET	'D,FALSE-VALUE
	EQUAL?	GL-WIN-N,K-MAP-BOG-TBL-NUM \?CCL17
	SET	'D,TRUE-VALUE
	JUMP	?CND15
